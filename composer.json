{
    "name": "mediumart/echo.io-php",
    "description": "php bindings for laravel echo.io server",
    "keywords": ["laravel-echo", "socket.io", "redis"],
    "license": "MIT",
    "authors": [
        {
            "name": "Isaac Esso",
            "email": "daprod2009@gmail.com"
        }
    ],
    "require": {
        "illuminate/broadcasting": "5.3.*|5.4.*|5.5.*|5.6.*",
        "firebase/php-jwt": "~5.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.3.*",
        "phpunit/phpunit": "~5.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Mediumart\\Echoio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mediumart\\Echoio\\Tests\\": "tests/"
        }
    },
     "extra": {
        "laravel": {
            "providers": [
                "Mediumart\\Echoio\\EchoioServiceProvider"
            ]
        }
    }
}
